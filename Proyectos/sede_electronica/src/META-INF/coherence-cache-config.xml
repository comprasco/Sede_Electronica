<?xml version="1.0"?>

<cache-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xmlns="http://xmlns.oracle.com/coherence/coherence-cache-config"
              xsi:schemaLocation="http://xmlns.oracle.com/coherence/coherence-cache-config http://xmlns.oracle.com/coherence/coherence-cache-config/1.2/coherence-cache-config.xsd">
	<caching-scheme-mapping>
		<cache-mapping>
			<cache-name>cache-snr</cache-name>
			<scheme-name>distributed</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>cache-snr-filtro-url</cache-name>
			<scheme-name>distributedFiltrosUrl</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>cache-snr-pais</cache-name>
			<scheme-name>distributedPais</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>cache-snr-tipo-documento</cache-name>
			<scheme-name>distributedTipoDocumento</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>cache-snr-estado-cuenta</cache-name>
			<scheme-name>distributedEstadoCuenta</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>cache-snr-tipo-proceso</cache-name>
			<scheme-name>distributedTiposProcesos</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>cache-snr-rol</cache-name>
			<scheme-name>distributedRol</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>cache-snr-usuario</cache-name>
			<scheme-name>distributedUsuario</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>cache-snr-rol-permisos</cache-name>
			<scheme-name>distributedRolesPermiso</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>cache-snr-sesion-principal</cache-name>
			<scheme-name>distributedPrincipal</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>cache-snr-sesion</cache-name>
			<scheme-name>distributedSesiones</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>cache-snr-ee-dummy</cache-name>
			<scheme-name>distributedEntidadesEspeciales</scheme-name>
		</cache-mapping>
	</caching-scheme-mapping>

	<caching-schemes>

		<distributed-scheme>
			<scheme-name>distributed</scheme-name>
			<service-name>DistributedCache</service-name>
			<backing-map-scheme>
				<local-scheme />
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>
		
		<distributed-scheme>
			<scheme-name>distributedEntidadesEspeciales</scheme-name>
			<service-name>DistributedCache</service-name>
			<backing-map-scheme>
				<local-scheme />
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>

		<distributed-scheme>
			<scheme-name>distributedPrincipal</scheme-name>
			<service-name>DistributedCacheSesion</service-name>
			<backing-map-scheme>
				<local-scheme />
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>
		<distributed-scheme>
			<scheme-name>distributedSesiones</scheme-name>
			<service-name>DistributedCacheSesion</service-name>
			<backing-map-scheme>
				<local-scheme />
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>

		<distributed-scheme>
			<scheme-name>distributedRolesPermiso</scheme-name>
			<service-name>DistributedCache</service-name>
			<backing-map-scheme>
				<local-scheme />
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>
		<distributed-scheme>
			<scheme-name>distributedUsuario</scheme-name>
			<service-name>DistributedCache</service-name>
			<backing-map-scheme>
				<local-scheme />
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>

		<distributed-scheme>
			<scheme-name>distributedFiltrosUrl</scheme-name>
			<service-name>DistributedCache</service-name>
			<backing-map-scheme>
				<local-scheme />
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>

		<distributed-scheme>
			<scheme-name>distributedPais</scheme-name>
			<service-name>DistributedCache</service-name>
			<backing-map-scheme>
				<local-scheme />
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>

		<distributed-scheme>
			<scheme-name>distributedTipoDocumento</scheme-name>
			<service-name>DistributedCache</service-name>
			<backing-map-scheme>
				<local-scheme />
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>


		<distributed-scheme>
			<scheme-name>distributedEstadoCuenta</scheme-name>
			<service-name>DistributedCache</service-name>
			<backing-map-scheme>
				<local-scheme />
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>

		<distributed-scheme>
			<scheme-name>distributedTiposProcesos</scheme-name>
			<service-name>DistributedCache</service-name>
			<backing-map-scheme>
				<local-scheme />
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>

		<distributed-scheme>
			<scheme-name>distributedRol</scheme-name>
			<service-name>DistributedCache</service-name>
			<backing-map-scheme>
				<local-scheme />
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>
	</caching-schemes>


</cache-config>